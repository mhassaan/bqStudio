<section id="content" class="m-t-lg wrapper-md animated fadeInUp">
    <div class="container aside-xxl">
      <a class="navbar-brand block" href="index.html">Notebook</a>
      <section class="panel panel-default bg-white m-t-lg">
        <header class="panel-heading text-center">
          <strong>Sign in</strong>
        </header>
        <%= form_for(resource, as: resource_name, url: session_path(resource_name), :html=>{ class: 'panel-body wrapper-lg'}) do |f| %>
            <div class="form-group">
              <%= f.label :email,class: 'control-label' %><br />
              <%= f.email_field :email, autofocus: true, class: 'form-control input-lg',placeholder: 'test@example.com' %>
            </div>
            <div class="form-group">
              <%= f.label :password,class: 'control-label' %><br />
              <%= f.password_field :password, autocomplete: "off",class: 'form-control input-lg',placeholder: 'Password' %>
            </div>
            <div class="line line-dashed"></div>
          <button type="submit" class="btn btn-primary btn-block">Sign in</button>
          <% end %>
      </section>
    </div>
  </section>
  <!-- footer -->
  <footer id="footer">
    <div class="text-center padder">
      <p>
        <small>Web app framework base on Bootstrap<br>&copy; 2013</small>
      </p>
    </div>
  </footer>

